// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id        String   @id @default(uuid())
  slug      String   @unique
  name      String
  wifiSsid  String?
  wifiPass  String?
  rules     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
